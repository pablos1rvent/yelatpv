<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yelatpv">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Inicio.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/CargaInicio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Principal.Home"
            android:screenOrientation="portrait"
            android:theme="@style/Home"/>
        <activity android:name=".Inicio.RegistroEmpresa"
            android:screenOrientation="portrait"
            android:theme="@style/RegistroEmpresa"/>
        <activity android:name=".Wizard.WizardEmpresa"
            android:screenOrientation="portrait"
            android:theme="@style/WizardEmpresa"/>
        <activity android:name=".Acciones.NuevaCategoria"
            android:screenOrientation="portrait"
            android:theme="@style/NuevaCategoria"/>
        <activity android:name=".Acciones.NuevoProducto"
            android:screenOrientation="portrait"
            android:theme="@style/NuevoProducto"/>
        <activity android:name=".Acciones.VerTicket"
            android:screenOrientation="portrait"
            android:theme="@style/VerTicket"/>
        <activity android:name=".Acciones.NuevaFidelizacion"
            android:screenOrientation="portrait"
        android:theme="@style/NuevaFidelizacion"/>
        <activity android:name=".Principal.Login"
            android:screenOrientation="portrait"
            android:theme="@style/Login"/>
        <activity android:name=".Acciones.NuevoUsuario"
            android:screenOrientation="portrait"
            android:theme="@style/NuevoUsuario"/>
    </application>

</manifest>